{
  "network": {
    "port": 8080,
    "address": "127.0.0.1"
  },

  "root": "./assets",
  "shutdown": "/admin/shutdown",
  "password": "admin-passwd",

  "address-mapping": {
    "/admin": "/admin",
    "/log": "/log",
    "/scripts": "/scripts",
    "/": "/client"
  },

  "restricted": [
    "/scripts",
    "/admin",
    "/log"
  ],

  "post": "/client/uploads/",

  "log": {
    "console": true,
    "file": false
  },

  "scripts": {
    ".c": "g++ path; path/a.out",
    ".cpp": "g++ path; path/a.out",
    ".sh": "path",
    ".py": "python3 path"
  }

}